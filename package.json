{
  "name": "PowerManagement",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "build": {
    "appId": "your.id",
    "productName": "Power Management",
    "extraResources": [
      {
        "filter": [
          "./setting.json"
        ]
      }
    ],
    "linux": {
      "target": "deb",
      "icon": "./icon.png"
    }
  },
  "scripts": {
    "start": "electron .",
    "dev": "nodemon --exec electron .",
    "app:dir": "electron-builder --dir",
    "app:dist": "electron-builder"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": {
    "email": "duchoang206h@gmail.com",
    "name": "Duc Hoang"
  },
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^21.2.1",
    "electron-builder": "^23.6.0",
    "nodemon": "^2.0.20"
  },
  "dependencies": {
    "battery": "^1.0.1",
    "cron": "^2.1.0",
    "dotenv": "^16.0.3",
    "electron-store": "^8.1.0",
    "mongoose": "^6.7.0"
  }
}
